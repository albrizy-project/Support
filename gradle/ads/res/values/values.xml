<?xml version="1.0" encoding="utf-8"?>
<resources>

    <string name="ad_id"/>
    <integer name="ad_interstitial_show_after_clicks">4</integer>
    <integer name="ad_hide_duration">15000</integer>

    <declare-styleable name="AdLayout">
        <attr name="ad_type" format="enum">
